{
  "name": "@autifyhq/autify-cli",
  "version": "0.7.1",
  "description": "Autify Command Line Interface (CLI)",
  "author": "Autify",
  "bin": {
    "autify": "./bin/run"
  },
  "homepage": "https://github.com/autifyhq/autify-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "autifyhq/autify-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@autifyhq/autify-sdk": "^0.3.0",
    "@oclif/core": "^1",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-update": "^3.0.0",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "envfile": "^6.17.0",
    "inquirer": "^8.2.4",
    "listr": "^0.14.3",
    "node-emoji": "^1.11.0",
    "per-env": "^1.0.2",
    "tempy": "^3.0.0",
    "tsimportlib": "^0.0.3",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@autifyhq/autify-cli-integration-test": "*",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.52",
    "@types/node-emoji": "^1.8.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-morph": "^14.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "autify",
    "dirname": "autify",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "topicSeparator": " ",
    "topics": {
      "web": {
        "description": "Autify for Web"
      },
      "web:api": {
        "description": "Low-level APIs on Autify for Web"
      },
      "web:auth": {
        "description": "Authentication on Autify for Web"
      },
      "web:test": {
        "description": "Test on Autify for Web"
      },
      "mobile": {
        "description": "Autify for Mobile"
      },
      "mobile:api": {
        "description": "Low-level APIs on Autify for Mobile"
      },
      "mobile:auth": {
        "description": "Authentication on Autify for Mobile"
      }
    },
    "update": {
      "s3": {
        "bucket": "autify-cli-assets",
        "folder": "autify-cli"
      }
    },
    "macos": {
      "identifier": "com.autify.cli"
    }
  },
  "scripts": {
    "postinstall": "per-env || true",
    "postinstall:development": "patch-package",
    "postinstall:production": "true",
    "generate:api-commands": "ts-node scripts/generate-api-commands.ts",
    "generate": "npm run generate:api-commands web && npm run generate:api-commands mobile && npm run format-lint-api-commands",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "format": "prettier . --write",
    "format-api-commands": "prettier --write src/commands/web/api src/commands/mobile/api",
    "lint-api-commands": "eslint --fix src/commands/web/api src/commands/mobile/api",
    "format-lint-api-commands": "npm run format-api-commands && npm run lint-api-commands",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "release": "ts-node ./scripts/release.ts",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:integration": "npm run build -w integration-test && autify-cli-integration-test",
    "test:integration:record": "npm run build -w integration-test && autify-cli-integration-test --record",
    "version": "oclif readme && git add README.md",
    "prepare": "per-env || true",
    "prepare:development": "husky install",
    "prepare:production": "true"
  },
  "lint-staged": {
    "**/*.{md,json,yml,js,cjs}": "prettier --write",
    "**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/autifyhq/autify-cli/issues",
  "keywords": [
    "oclif"
  ],
  "workspaces": [
    "integration-test"
  ],
  "types": "dist/index.d.ts"
}
